{
  "name": "book",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^4.1.3",
    "coveralls": "^3.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.5",
    "react-toastify": "^4.2.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/actions/responseInterceptor.js",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "react-test-renderer": "^16.5.0",
    "redux-mock-store": "^1.5.3"
  },
  "proxy": "http://127.0.0.1:5000",
  "jest": {
    "setupTestFrameworkScriptFile": "./src/__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "./src/__tests__/setup/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif)$": "./src/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./src/__mocks__/browserMock.js"
    ]
  }
}
